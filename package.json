    "@types/ramda": "0.26.38",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "dependency-cruiser": "6.0.0",
    "eslint": "6.8.0",
    "typescript": "3.7.4"
